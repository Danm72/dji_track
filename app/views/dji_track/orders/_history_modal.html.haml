#historyModal.modal.fade{ tabindex: '-1', role: 'dialog', 'aria-labelledby' => 'historyModalLabel', 'aria-hidden' => 'true' }
  .modal-dialog{ role: 'document' }
    .modal-content
      .modal-header
        %button.close{ type: 'button', 'data-dismiss' => 'modal', 'aria-label' => 'Close' }
          %span{ 'aria-hidden' => 'true' }
            &times;
        %h4#historyModalLabel.modal-title
          Change History
      .modal-body
        
      .modal-footer
        %button.btn.btn-secondary{ type: 'button', 'data-dismiss' => 'modal' }
          Close
